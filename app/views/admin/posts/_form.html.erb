<%= form_with model: [:admin, post] do |f| %>
  <%= render 'layouts/error_messages', model: f.object %>
  <h4>画像</h4>
  <%= f.file_field :image, accept: "image/*" %>
  <h4>イベント名</h4>
  <%= f.text_field :title, style: "width: 400px;" %>
  <h4>説明</h4>
  <%= f.text_area :body, rows: 10, cols: 40 %>
  <p><%= f.submit '投稿' %></p>
<% end %>